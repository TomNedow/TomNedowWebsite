    this is where my working add to hidden workout code goes
    function addExerciseToWorkout() {
        const exerciseStats = document.getElementById("exercise-stats");
        const exerciseContainer = document.getElementById("program-container");
    
        const nameElement = exerciseStats?.querySelector("#name");
        const categoryElement = exerciseStats?.querySelector("#category");
        const noteElement = exerciseStats?.querySelector("#exercise-notes");
    
        const selectedExerciseName = nameElement?.textContent.trim();
        const selectedExerciseCategory = categoryElement?.textContent.trim();
        const selectedExerciseNotes = noteElement?.textContent.trim();
    
        if (selectedExerciseName) {
            // Create a container div for the new exercise info
            const newDiv = document.createElement('div');
            newDiv.classList.add("program-div");
            
            // Create separate elements for each piece of information
            const exerciseNameDiv = document.createElement('h1');
            exerciseNameDiv.textContent = `Exercise: ${selectedExerciseName}`;
            exerciseNameDiv.classList.add("program-h1"); 
    
            const categoryDiv = document.createElement('p');
            categoryDiv.textContent = `Category: ${selectedExerciseCategory}`;
            categoryDiv.classList.add("program-category")
    
            const noteDiv = document.createElement('p');
            noteDiv.textContent = `Note: ${selectedExerciseNotes}`;
            noteDiv.style.color = "#555"; 
    
            newDiv.appendChild(exerciseNameDiv);
            newDiv.appendChild(categoryDiv);
            newDiv.appendChild(noteDiv);
    
            // Append the newDiv to the exerciseContainer
            exerciseContainer.appendChild(newDiv);
        } else {
            alert("Please select an exercise first.");
        }
    }




    // Create a container div for the new exercise info
        const newDiv = document.createElement('div');
        newDiv.classList.add("program-div");
        const exerciseNameDiv = document.createElement('h1');
        exerciseNameDiv.textContent = `${selectedExerciseName}`;
        exerciseNameDiv.classList.add("program-h1");
        const categoryDiv = document.createElement('p');
        categoryDiv.textContent = `${selectedExerciseCategory}`;
        categoryDiv.classList.add("program-category");
        const noteDiv = document.createElement('p');
        noteDiv.textContent = `${selectedExerciseNotes}`;
        noteDiv.style.color = "#555";

        // Append exercise info to the newDiv
        newDiv.appendChild(exerciseNameDiv);
        newDiv.appendChild(categoryDiv);
        newDiv.appendChild(noteDiv);

        // Add sets and reps to the exercise
        const sets = document.getElementById("sets");
        const reps = document.getElementById("reps");
        const setsValue = sets?.value.trim();
        const repsValue = reps?.value.trim();

        if (setsValue && repsValue) {
            const setsRepsDiv = document.createElement("div");
            setsRepsDiv.classList.add("program-reps-div");

            const repsText = document.createElement("p");
            repsText.textContent = ` ${setsValue} x ${repsValue}`;
            repsText.classList.add("program-reps");

            setsRepsDiv.appendChild(repsText);
            newDiv.appendChild(setsRepsDiv);
        }
        // Append the newDiv to the exerciseContainer
        exerciseContainer.appendChild(newDiv);

    } else {
        alert("Please select an exercise first.");
    }
}



//counter function
    
let countIndex = 0;

const repCounter = () => {
    const repCounterContainer = document.getElementById("rep-counter-container");
    let userRepsInput = repsInput.value;
    let repNumber = document.querySelector(".rep-counter-div")
    //
    for (let i = 0; i < userRepsInput; i++){
        repNumber.innerHTML = `<h1>${countIndex++} out of ${userRepsInput}</h1>`
} else{

}}

startSetBtn.addEventListener("click",() => {
    
    setInterval(repCounter, 1000)
})



